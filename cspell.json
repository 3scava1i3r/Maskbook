{
    "$schema": "https://github.com/streetsidesoftware/cspell/raw/main/cspell.schema.json",
    "dictionaries": ["typescript", "node", "html", "css", "fonts", "web3-tokens"],
    "dictionaryDefinitions": [
        {
            "name": "web3-tokens",
            "path": "packages/web3-constants/tokens.txt"
        }
    ],
    "useGitignore": true,
    "cache": {
        "useCache": true
    },
    "ignorePaths": [
        "!locales/en-us.json",
        ".*ignore",
        ".eslintrc.json",
        ".github",
        ".lintstagedrc.mjs",
        ".prettierignore",
        "*.d.ts",
        "*.html",
        "*.json",
        "*.snap",
        "*generated*",
        "dist",
        "locales/*.json",
        "node_modules",
        "package.json",
        "packages/mask/src/plugins/ito/assets/regions.json",
        "packages/mask/src/resources",
        "packages/web3-contracts/{abis,types}",
        "packages/web3-shared/evm/assets/**/*.json",
        "patches/**",
        "pnpm-lock.yaml"
    ],
    "words": [
        "aave",
        "adai",
        "apikey",
        "arbitrum",
        "avax",
        "bgcolor",
        "bignumber",
        "bips",
        "blockie",
        "blockies",
        "boba",
        "caip",
        "checksummed",
        "codegen",
        "commentid",
        "contenteditable",
        "contrib",
        "contribs",
        "cooldown",
        "debank",
        "destory",
        "dimensiondev",
        "dompurify",
        "endregion",
        "ethersproject",
        "filelist",
        "geckoview",
        "holoflows",
        "immer",
        "investables",
        "ipfs",
        "ipfsurl",
        "ittr",
        "keccak",
        "markdownlint",
        "maskbook",
        "masknet",
        "masknetwork",
        "mathwallet",
        "metaverse",
        "msgpack",
        "multicall",
        "multihop",
        "newsfeed",
        "nft",
        "nftscan",
        "opensea",
        "overridable",
        "overscan",
        "pageable",
        "pangolin",
        "popper",
        "proxied",
        "qrcode",
        "rarible",
        "realmasknetwork",
        "resizer",
        "secp",
        "solana",
        "steganography",
        "swappable",
        "swither",
        "tabindex",
        "tablist",
        "testid",
        "thegraph",
        "tokenid",
        "transak",
        "typeson",
        "unstyled",
        "urlcat",
        "walletconnect",
        "walletlink",
        "wavax",
        "webextension",
        "zerion"
    ],
    "ignoreWords": ["hookform", "locationchange", "onflow", "usei18n"],
    "ignoreRegExpList": ["/@servie/"],
    "overrides": [
        {
            "filename": "**/*.md",
            "ignoreWords": ["guanbinrui", "pnpm", "pnpx", "yisiliu", "zhouhancheng"]
        },
        {
            "filename": "packages/encryption",
            "ignoreWords": ["algr", "steganographic", "stego"]
        },
        {
            "filename": "packages/mask/{src,background}/database",
            "ignoreWords": ["idbp"]
        },
        {
            "filename": "packages/mask/src/plugins/Collectible",
            "ignoreWords": ["enjin", "unreviewed"]
        },
        {
            "filename": "packages/mask/src/plugins/CryptoartAI",
            "ignoreWords": ["avator", "cryptoart", "cryptoartai", "wnative"]
        },
        {
            "filename": "packages/mask/src/plugins/dHEDGE",
            "ignoreWords": ["dhedge"]
        },
        {
            "filename": "packages/mask/src/plugins/FindTruman",
            "ignoreWords": ["consts", "findtruman", "poap", "poaps", "rawdata"]
        },
        {
            "filename": "packages/mask/src/plugins/Furucombo",
            "ignoreWords": ["furucombo", "wmatic"]
        },
        {
            "filename": "packages/mask/src/plugins/GoodGhosting",
            "ignoreWords": ["datas", "wnative"]
        },
        {
            "filename": "packages/mask/src/plugins/ITO",
            "ignoreWords": ["addrs", "amountstate", "defi", "gamification", "pids", "powahs"]
        },
        {
            "filename": ["packages/plugins/Flow", "packages/web3-shared/flow"],
            "ignoreWords": ["blocto"]
        },
        {
            "filename": "packages/mask/src/plugins/MaskBox",
            "ignoreWords": ["drawed"]
        },
        {
            "filename": "packages/mask/src/plugins/PoolTogether",
            "ignoreWords": ["pooltogether", "timelock"]
        },
        {
            "filename": "packages/mask/src/plugins/RedPacket",
            "ignoreWords": ["ifclaimed", "nftred", "redpacket", "rpid", "txid"]
        },
        {
            "filename": "packages/mask/src/plugins/Snapshot",
            "ignoreWords": ["millify"]
        },
        {
            "filename": "packages/mask/src/plugins/Trader",
            "ignoreWords": [
                "cashtag",
                "datas",
                "energi",
                "jsbi",
                "openocean",
                "pancakeswap",
                "quickswap",
                "sashimiswap",
                "sushiswap",
                "traderjoe",
                "trisolaris",
                "wannaswap",
                "wnative"
            ]
        },
        {
            "filename": "packages/mask/src/plugins/UnlockProtocol",
            "ignoreWords": ["chainid", "unlocklock", "unlockprotocol"]
        },
        {
            "filename": "packages/mask/src/plugins/VCent",
            "ignoreWords": ["vcent"]
        },
        {
            "filename": "packages/mask/src/plugins/Wallet",
            "ignoreWords": ["cipherparams", "ciphertext", "dklen", "kdfparams", "metaswap", "redpacket", "unstake"]
        },
        {
            "filename": "packages/plugins/FileService",
            "ignoreWords": ["arweave", "fileservice", "unencrypted"]
        },
        {
            "filename": "packages/plugins/RSS3",
            "ignoreWords": ["poap"]
        },
        {
            "filename": ["packages/plugins/Solana", "packages/web3-shared/solana"],
            "ignoreWords": ["devnet", "metaplex"]
        },
        {
            "filename": "packages/web3-providers",
            "ignoreWords": ["poap", "unstake"]
        }
    ]
}
