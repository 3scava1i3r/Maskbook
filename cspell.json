{
    "$schema": "https://github.com/streetsidesoftware/cspell/raw/main/cspell.schema.json",
    "dictionaries": ["typescript", "node", "html", "css", "fonts", "web3-tokens", "trader-tokens"],
    "dictionaryDefinitions": [
        {
            "name": "web3-tokens",
            "path": "packages/web3-constants/tokens.txt"
        },
        {
            "name": "trader-tokens",
            "path": "packages/mask/src/plugins/Trader/constants/tokens.txt"
        }
    ],
    "useGitignore": true,
    "cache": {
        "useCache": true
    },
    "ignorePaths": [
        ".*ignore",
        ".github",
        ".lintstagedrc.mjs",
        ".prettierignore",
        "*.snap",
        "**/.eslintrc.json",
        "**/*.d.ts",
        "**/*.html",
        "**/*.json",
        "**/dist/**",
        "**/node_modules/**",
        "**/package.json",
        "**/pnpm-lock.yaml",
        "**/qya-aa.json",
        "*generated*",
        "packages/mask/src/plugins/ito/assets/regions.json",
        "packages/mask/src/resources",
        "packages/web3-contracts/{abis,types}",
        "patches/**",
        "pnpm-lock.yaml"
    ],
    "words": [
        "aave",
        "adai",
        "apikey",
        "arbitrum",
        "avax",
        "bgcolor",
        "bignumber",
        "bips",
        "blockie",
        "blockies",
        "boba",
        "caip",
        "checksummed",
        "codegen",
        "commentid",
        "contenteditable",
        "contrib",
        "contribs",
        "cooldown",
        "debank",
        "destory",
        "dimensiondev",
        "dompurify",
        "endregion",
        "ethersproject",
        "filelist",
        "geckoview",
        "holoflows",
        "immer",
        "investables",
        "ipfs",
        "ipfsurl",
        "ittr",
        "keccak",
        "markdownlint",
        "maskbook",
        "masknet",
        "masknetwork",
        "mathwallet",
        "metaverse",
        "msgpack",
        "multicall",
        "multihop",
        "newsfeed",
        "nftscan",
        "opensea",
        "overridable",
        "overscan",
        "pageable",
        "pangolin",
        "popper",
        "proxied",
        "qrcode",
        "rarible",
        "realmasknetwork",
        "resizer",
        "secp",
        "solana",
        "steganography",
        "swappable",
        "swither",
        "tabindex",
        "tablist",
        "testid",
        "thegraph",
        "tokenid",
        "transak",
        "typeson",
        "unstyled",
        "urlcat",
        "walletconnect",
        "walletlink",
        "wavax",
        "webextension",
        "zerion"
    ],
    "ignoreWords": [
        "avator",
        "blocto",
        "chainid",
        "defi",
        "devnet",
        "ethjs",
        "hookform",
        "iife",
        "locationchange",
        "nft",
        "onflow",
        "usei18n",
        "wmatic",
        "wnative"
    ],
    "ignoreRegExpList": ["/@servie/"],
    "overrides": [
        {
            "filename": "**/*.md",
            "ignoreWords": ["guanbinrui", "pnpm", "pnpx", "yisiliu", "zhouhancheng"]
        },
        {
            "filename": "packages/encryption",
            "ignoreWords": ["algr", "steganographic", "stego"]
        },
        {
            "filename": "packages/web3-shared/evm/assets/**/*.json",
            "ignoreWords": ["g√∂rli"]
        },
        {
            "filename": "packages/web3-providers",
            "ignoreWords": ["poap", "unstake"]
        },
        {
            "filename": "packages/mask/{src,background}/database",
            "ignoreWords": ["idbp"]
        },
        {
            "filename": "packages/mask/src/plugins/RedPacket",
            "ignoreWords": ["ifclaimed", "nftred", "redpacket", "rpid", "txid"]
        },
        {
            "filename": "packages/mask/src/plugins/UnlockProtocol",
            "ignoreWords": ["unlocklock", "unlockprotocol"]
        },
        {
            "filename": "packages/mask/src/plugins/Wallet",
            "ignoreWords": ["cipherparams", "ciphertext", "dklen", "kdfparams", "metaswap", "redpacket", "unstake"]
        },
        {
            "filename": "packages/mask/src/plugins/FindTruman",
            "ignoreWords": ["consts", "findtruman", "poap", "poaps", "rawdata"]
        },
        {
            "filename": "packages/mask/src/plugins/GoodGhosting",
            "ignoreWords": ["datas"]
        },
        {
            "filename": "packages/mask/src/plugins/ITO",
            "ignoreWords": ["addrs", "amountstate", "gamification", "pids", "powahs"]
        },
        {
            "filename": "packages/mask/src/plugins/Trader",
            "ignoreWords": [
                "cashtag",
                "datas",
                "energi",
                "jsbi",
                "openocean",
                "pancakeswap",
                "quickswap",
                "sashimiswap",
                "sushiswap",
                "traderjoe",
                "trisolaris",
                "wannaswap"
            ]
        },
        {
            "filename": "packages/mask/src/plugins/PoolTogether",
            "ignoreWords": ["pooltogether", "timelock"]
        },
        {
            "filename": "packages/mask/src/plugins/Furucombo",
            "ignoreWords": ["furucombo"]
        },
        {
            "filename": "packages/mask/src/plugins/dHEDGE",
            "ignoreWords": ["dhedge"]
        },
        {
            "filename": "packages/mask/src/plugins/Collectible",
            "ignoreWords": ["enjin", "unreviewed"]
        },
        {
            "filename": "packages/mask/src/plugins/MaskBox",
            "ignoreWords": ["drawed"]
        },
        {
            "filename": "packages/mask/src/plugins/VCent",
            "ignoreWords": ["vcent"]
        },
        {
            "filename": "packages/plugins/FileService",
            "ignoreWords": ["arweave", "fileservice", "unencrypted"]
        },
        {
            "filename": "packages/mask/src/plugins/CryptoartAI",
            "ignoreWords": ["cryptoart", "cryptoartai"]
        },
        {
            "filename": "packages/mask/src/plugins/Snapshot",
            "ignoreWords": ["millify"]
        },
        {
            "filename": "packages/plugins/RSS3",
            "ignoreWords": ["poap"]
        },
        {
            "filename": "packages/plugins/Solana",
            "ignoreWords": ["metaplex"]
        }
    ]
}
